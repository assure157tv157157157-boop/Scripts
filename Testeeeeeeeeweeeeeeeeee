-- N U K E
TrollTab:AddSection({"Nuke"})

local skyboxEnabled = false
local skyboxTrack = nil
local rigidTrack = nil
local savedNukeBody = {}

local function stopAllAnimations()
    if rigidTrack then
        pcall(function()
            rigidTrack:Stop()
            rigidTrack:Destroy()
        end)
        rigidTrack = nil
    end

    if skyboxTrack then
        pcall(function()
            skyboxTrack:Stop()
            skyboxTrack:Destroy()
        end)
        skyboxTrack = nil
    end
end

TrollTab:AddToggle({
    Name = "Nuke",
    Default = false,
    Callback = function(value)
        skyboxEnabled = value

        if value then
            local description = humanoid:GetAppliedDescription()

            savedNukeBody = {
                Torso = description.Torso,
                RightArm = description.RightArm,
                LeftArm = description.LeftArm,
                RightLeg = description.RightLeg,
                LeftLeg = description.LeftLeg,
                Head = description.Head
            }

            task.wait(0.2)

            local args = {
                123402086843885,
                100839513065432,
                78300682916056,
                86276701020724,
                78409653958165,
                15093053680
            }

            ReplicatedStorage.Remotes.ChangeCharacterBody:InvokeServer(args)

            task.wait(0.3)

            local anim1 = Instance.new("Animation")
            anim1.AnimationId = "rbxassetid://70883871260184"
            skyboxTrack = humanoid:LoadAnimation(anim1)
            skyboxTrack.Priority = Enum.AnimationPriority.Action4
            skyboxTrack:Play(0.1, 1, 0.01)

            task.wait(0.5)

            local anim2 = Instance.new("Animation")
            anim2.AnimationId = "rbxassetid://3695333486"
            rigidTrack = humanoid:LoadAnimation(anim2)
            rigidTrack.Priority = Enum.AnimationPriority.Movement
            rigidTrack:Play()
        else
            stopAllAnimations()

            if next(savedNukeBody) then
                ReplicatedStorage.Remotes.ChangeCharacterBody:InvokeServer({
                    savedNukeBody.Torso,
                    savedNukeBody.RightArm,
                    savedNukeBody.LeftArm,
                    savedNukeBody.RightLeg,
                    savedNukeBody.LeftLeg,
                    savedNukeBody.Head
                })
                savedNukeBody = {}
            end
        end
    end
})

