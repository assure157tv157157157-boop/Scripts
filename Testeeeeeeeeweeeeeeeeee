--==================================================
-- FUNÃ‡ÃƒO BASE (NÃƒO MEXER)
--==================================================
local function ApplyLegs(rightLegId, leftLegId)
    if not humanoid then return end

    local desc = humanoid:GetAppliedDescription()

    ChangeBody:InvokeServer({
        desc.Torso,
        desc.RightArm,
        desc.LeftArm,
        rightLegId or desc.RightLeg,
        leftLegId  or desc.LeftLeg,
        0
    })
end

