                    [6] = _G.savedBodyParts.Head
                }

                pcall(function()
                    ReplicatedStorage.Remotes.ChangeCharacterBody:InvokeServer(restoreBody)
                end)

                _G.savedBodyParts = {}
            end
        end
    end
})


