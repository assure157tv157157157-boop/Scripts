            if next(_G.savedBodyParts) then
                local restoreBody = {
                    [1] = _G.savedBodyParts.Torso,
                    [2] = _G.savedBodyParts.RightArm,
                    [3] = _G.savedBodyParts.LeftArm,
                    [4] = _G.savedBodyParts.RightLeg,
